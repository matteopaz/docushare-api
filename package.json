{
    "name": "worker-typescript-template",
    "version": "1.0.0",
    "description": "Cloudflare worker TypeScript template",
    "main": "dist/worker.js",
    "type": "module",
    "scripts": {
        "dev": "miniflare --build-watch-path src --kv USERS --kv DOCS --build-command=\"npm run devbuild\" --binding JWT_SECRET_KEY=989htnugre87m880qmjdiu2ry80qyv0eywtq",
        "devbuild": "node build_dev.js",
        "build": "tsc --noEmit && node build_hard.js",
        "format": "prettier --write *.{json,js} src/*.{js,ts} src/**/*.{js,ts} test/**/*.{js,ts}",
        "lint": "eslint --max-warnings=0 src/* && prettier --check *.{json,js} src/*.{js,ts} src/**/*.{js,ts} test/**/*.{js,ts} && tsc --noEmit",
        "test": "jest --config jestconfig.json --verbose"
    },
    "author": "author",
    "license": "MIT OR Apache-2.0",
    "eslintConfig": {
        "root": true,
        "extends": [
            "typescript",
            "prettier"
        ]
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^2.2.2",
        "@tsndr/cloudflare-worker-jwt": "^1.1.4",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.6",
        "@types/service-worker-mock": "^2.0.1",
        "@typescript-eslint/eslint-plugin": "^4.16.1",
        "@typescript-eslint/parser": "^4.16.1",
        "esbuild": "^0.13.7",
        "jest": "^27.2.5",
        "prettier": "^2.3.0",
        "service-worker-mock": "^2.0.5",
        "ts-jest": "^27.0.7",
        "typescript": "^4.3.2"
    },
    "dependencies": {
        "itty-router": "^2.4.4"
    }
}